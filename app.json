{
  "expo": {
    "name": "LuminaNDriver",
    "slug": "luminandriver",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "luminandriver",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.luminandriver"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.luminandriver"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-asset",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/splash-icon.png"
        }
      ],
      "react-native-edge-to-edge",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location for order tracking."
        }
      ]
    ],
    "owner": "luminan",
    "extra": {
      "eas": {
        "projectId": "9e6fc973-9a12-46a4-b334-c65e80f92b9d"
      }
    }
  }
}
